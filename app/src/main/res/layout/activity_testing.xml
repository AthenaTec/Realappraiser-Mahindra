<?xml version="1.0" encoding="utf-8"?><!--suppress ALL -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linear_more_details"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_5"
            android:orientation="horizontal"
            android:weightSum="3"
            tools:ignore="DisableBaselineAlignment">

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_municipal_ward"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingRight="@dimen/height_5"
                tools:ignore="RtlHardcoded,RtlSymmetry">

                <EditText
                    android:id="@+id/editText_municipal_ward"
                    style="@style/StyledTilEditText"
                    android:hint="@string/municipal_ward"
                    android:inputType="textCapWords"
                    android:maxLength="100" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_taluka_mandal_tehsil"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingLeft="@dimen/height_5"
                android:paddingRight="@dimen/height_5">

                <EditText
                    android:id="@+id/editText_taluka_mandal_tehsil"
                    style="@style/StyledTilEditText"
                    android:hint="@string/taluka_mandal_tehsil"
                    android:inputType="textCapWords"
                    android:maxLength="100" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_villagename"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingLeft="@dimen/height_5"
                tools:ignore="RtlHardcoded,RtlSymmetry">

                <EditText
                    android:id="@+id/editText_villagename"
                    style="@style/StyledTilEditText"
                    android:hint="@string/villagename"
                    android:inputType="textCapWords"
                    android:maxLength="100" />
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="3"
            tools:ignore="DisableBaselineAlignment">

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_district"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingRight="@dimen/height_5"
                tools:ignore="RtlHardcoded,RtlSymmetry">

                <EditText
                    android:id="@+id/editText_district"
                    style="@style/StyledTilEditText"
                    android:hint="@string/district"
                    android:inputType="textCapWords"
                    android:maxLength="100" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_surveyno"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingLeft="@dimen/height_5"
                android:paddingRight="@dimen/height_5">

                <EditText
                    android:id="@+id/editText_surveyno"
                    style="@style/StyledTilEditText"
                    android:hint="@string/surveyno"
                    android:inputType="textCapWords"
                    android:maxLength="50" />
            </com.google.android.material.textfield.TextInputLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/height_8"
                android:layout_marginRight="@dimen/height_3"
                android:layout_weight="0.35"
                android:background="@drawable/bottom_grey"
                tools:ignore="RtlHardcoded">

                <Spinner
                    android:id="@+id/spinner_nameofseller"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginRight="2dp"
                    android:background="@null"
                    android:spinnerMode="dialog"
                    tools:ignore="InconsistentLayout,RtlHardcoded" />

                <ImageView
                    style="@style/StyledImagedown_large"
                    android:layout_marginTop="6dp"
                    tools:ignore="ContentDescription" />
            </FrameLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_nameofseller"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <EditText
                    android:id="@+id/editText_nameofseller"
                    style="@style/StyledTilEditText"
                    android:hint="@string/nameofseller"
                    android:inputType="textCapWords"
                    android:maxLength="100" />
            </com.google.android.material.textfield.TextInputLayout>

            <!--<android.support.design.widget.TextInputLayout
                android:id="@+id/layout_nameofseller"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingLeft="@dimen/height_5">

                <EditText
                    android:id="@+id/editText_nameofseller"
                    style="@style/StyledTilEditText"
                    android:hint="@string/nameofseller"
                    android:inputType="textCapWords"
                    android:maxLength="100" />
            </android.support.design.widget.TextInputLayout>-->


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="3"
            tools:ignore="DisableBaselineAlignment">

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_unitno"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingRight="@dimen/height_5"
                tools:ignore="RtlHardcoded,RtlSymmetry">

                <EditText
                    android:id="@+id/editText_unitno"
                    style="@style/StyledTilEditText"
                    android:hint="@string/unitno"
                    android:inputType="textCapWords"
                    android:maxLength="10" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_ctsno"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingLeft="@dimen/height_5"
                android:paddingRight="@dimen/height_5">

                <EditText
                    android:id="@+id/editText_ctsno"
                    style="@style/StyledTilEditText"
                    android:hint="@string/ctsno"
                    android:inputType="textCapWords"
                    android:maxLength="50" />
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/layout_colony_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:paddingLeft="@dimen/height_5"
                tools:ignore="RtlHardcoded,RtlSymmetry">

                <EditText
                    android:id="@+id/editText_colony_name"
                    style="@style/StyledTilEditText"
                    android:hint="@string/colony_name"
                    android:inputType="textCapWords"
                    android:maxLength="50" />
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/more_info_land"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3"
                tools:ignore="DisableBaselineAlignment">

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/layout_wingno"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingRight="@dimen/height_5"
                    tools:ignore="RtlHardcoded,RtlSymmetry">

                    <EditText
                        android:id="@+id/editText_wingno"
                        style="@style/StyledTilEditText"
                        android:hint="@string/wingno_ka"
                        android:inputType="number"
                        android:maxLength="2" />
                </com.google.android.material.textfield.TextInputLayout>


                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/layout_year_of_current_tenancy"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/height_5"
                    android:paddingRight="@dimen/height_5">

                    <EditText
                        android:id="@+id/editText_year_of_current_tenancy"
                        style="@style/StyledTilEditText"
                        android:hint="@string/year_of_current_tenancy"
                        android:inputType="number"
                        android:maxLength="4" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/layout_amenities"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/height_5"
                    tools:ignore="RtlHardcoded,RtlSymmetry">

                    <EditText
                        android:id="@+id/editText_amenities"
                        style="@style/StyledTilEditText"
                        android:hint="@string/amenities"
                        android:inputType="textCapWords"
                        android:maxLength="150" />
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="3"
                tools:ignore="DisableBaselineAlignment">

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/layout_rentalincome"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingRight="@dimen/height_5"
                    tools:ignore="RtlHardcoded,RtlSymmetry">

                    <EditText
                        android:id="@+id/editText_rentalincome"
                        style="@style/StyledTilEditText"
                        android:hint="@string/rentalincome"
                        android:inputType="text"
                        android:maxLength="30" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/layout_wing_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/height_5"
                    android:paddingRight="@dimen/height_5">

                    <EditText
                        android:id="@+id/editText_wing_name"
                        style="@style/StyledTilEditText"
                        android:hint="@string/wing_name"
                        android:inputType="textCapWords"
                        android:maxLength="50" />
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/layout_floor_height"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingLeft="@dimen/height_5"
                    tools:ignore="RtlHardcoded,RtlSymmetry">

                    <EditText
                        android:id="@+id/editText_floor_height"
                        style="@style/StyledTilEditText"
                        android:hint="@string/floor_height"
                        android:inputType="textCapWords"
                        android:maxLength="18"
                        tools:ignore="TextFields" />
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/height_3"
                android:orientation="horizontal"
                android:weightSum="3"
                tools:ignore="DisableBaselineAlignment">

                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/height_1"
                    android:layout_marginRight="@dimen/height_5"
                    android:layout_weight="1"
                    android:background="@drawable/bottom_grey"
                    tools:ignore="RtlHardcoded">

                    <TextView
                        android:id="@+id/passagecorridorschowklobby"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/height_3"
                        android:gravity="left"
                        android:text="@string/passagecorridorschowklobby"
                        android:textColor="@color/font_text_minor_heading"
                        android:textSize="@dimen/miniut_small"
                        tools:ignore="RtlHardcoded" />

                    <Spinner
                        android:id="@+id/spinner_passagecorridorschowklobby"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        tools:ignore="InconsistentLayout" />

                    <ImageView
                        style="@style/StyledImagedown_large"
                        tools:ignore="ContentDescription" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/height_8"
                    android:layout_marginRight="@dimen/height_8"
                    android:layout_weight="1"
                    android:background="@drawable/bottom_grey"
                    tools:ignore="RtlHardcoded">

                    <TextView
                        android:id="@+id/amenitiesquality"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/height_3"
                        android:gravity="left"
                        android:text="@string/amenitiesquality"
                        android:textColor="@color/font_text_minor_heading"
                        android:textSize="@dimen/miniut_small"
                        tools:ignore="RtlHardcoded" />

                    <Spinner
                        android:id="@+id/spinner_amenitiesquality"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        tools:ignore="InconsistentLayout" />

                    <ImageView
                        style="@style/StyledImagedown_large"
                        tools:ignore="ContentDescription" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/height_5"
                    android:layout_marginRight="@dimen/height_1"
                    android:layout_weight="1"
                    android:background="@drawable/bottom_grey"
                    tools:ignore="RtlHardcoded">

                    <TextView
                        android:id="@+id/wateravailabilty"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/height_3"
                        android:gravity="left"
                        android:text="@string/wateravailabilty"
                        android:textColor="@color/font_text_minor_heading"
                        android:textSize="@dimen/miniut_small"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textview_wateravailabilty_text"
                        style="@style/StyledTilText_multicheck"
                        android:layout_gravity="left|center_vertical"
                        android:gravity="left|center_vertical"
                        tools:ignore="InconsistentLayout,RtlHardcoded" />


                    <ImageView
                        style="@style/StyledImagedown_large"
                        tools:ignore="ContentDescription" />
                </FrameLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/height_3"
                android:orientation="horizontal"
                android:weightSum="3"
                tools:ignore="DisableBaselineAlignment">

                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/height_1"
                    android:layout_marginRight="@dimen/height_5"
                    android:layout_weight="1"
                    android:background="@drawable/bottom_grey">

                    <TextView
                        android:id="@+id/pavingaroundbuilding"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/height_3"
                        android:gravity="left"
                        android:text="@string/pavingaroundbuilding"
                        android:textColor="@color/font_text_minor_heading"
                        android:textSize="@dimen/miniut_small"
                        tools:ignore="RtlHardcoded" />

                    <Spinner
                        android:id="@+id/spinner_pavingaroundbuilding"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@null"
                        tools:ignore="InconsistentLayout" />

                    <ImageView
                        style="@style/StyledImagedown_large"
                        tools:ignore="ContentDescription" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/height_8"
                    android:layout_marginRight="@dimen/height_8"
                    android:layout_weight="1"
                    android:background="@drawable/bottom_grey"
                    tools:ignore="RtlHardcoded">

                    <TextView
                        android:id="@+id/carparking"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/height_3"
                        android:gravity="left"
                        android:text="@string/carparking"
                        android:textColor="@color/font_text_minor_heading"
                        android:textSize="@dimen/miniut_small"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        android:id="@+id/textview_carparking_text"
                        style="@style/StyledTilText_multicheck"
                        android:layout_gravity="left|center_vertical"
                        android:gravity="left|center_vertical"
                        tools:ignore="InconsistentLayout,RtlHardcoded" />


                    <ImageView
                        style="@style/StyledImagedown_large"
                        tools:ignore="ContentDescription" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/height_5"
                    android:layout_marginRight="@dimen/height_1"
                    android:layout_weight="1"
                    android:background="@drawable/bottom_grey"
                    android:visibility="invisible"
                    tools:ignore="RtlHardcoded">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/height_3"
                        android:gravity="left"
                        android:text="@string/wateravailabilty"
                        android:textColor="@color/font_text_minor_heading"
                        android:textSize="@dimen/miniut_small"
                        tools:ignore="RtlHardcoded" />

                    <TextView
                        style="@style/StyledTilText_multicheck"
                        android:layout_gravity="left|center_vertical"
                        android:gravity="left|center_vertical"
                        tools:ignore="InconsistentLayout,RtlHardcoded" />

                    <ImageView
                        style="@style/StyledImagedown_large"
                        tools:ignore="ContentDescription" />
                </FrameLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/height_4"
                android:layout_marginTop="@dimen/height_4"
                android:orientation="horizontal"
                android:weightSum="2"
                tools:ignore="DisableBaselineAlignment">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.8"
                    android:orientation="vertical"
                    android:paddingLeft="@dimen/height_4"
                    tools:ignore="RtlHardcoded">

                    <TextView
                        android:id="@+id/textview_garden_exists"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerInParent="true"
                        android:layout_centerVertical="true"
                        android:layout_weight="1"
                        android:paddingLeft="@dimen/height_8"
                        android:text="@string/garden_exists"
                        android:textColor="@color/font_heading"
                        android:textSize="@dimen/font_samll"
                        android:textStyle="normal"
                        tools:ignore="RtlHardcoded" />

                    <RadioGroup
                        android:id="@+id/id_radiogroup_garden_exists"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_centerVertical="true"
                        android:layout_toRightOf="@+id/textview_garden_exists"
                        android:layout_weight="1"
                        android:orientation="horizontal"
                        tools:ignore="RtlHardcoded">

                        <RadioButton
                            android:id="@+id/id_radio_garden_exists_yes"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:checked="false"
                            android:text="@string/yes" />

                        <RadioButton
                            android:id="@+id/id_radio_garden_exists_no"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/height_15"
                            android:checked="false"
                            android:text="@string/no"
                            tools:ignore="RtlHardcoded" />
                    </RadioGroup>
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="right"
                    android:layout_weight="1.2">

                    <FrameLayout
                        android:layout_centerVertical="true"
                        android:id="@+id/type_of_compound_div"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true"
                        android:layout_marginLeft="@dimen/height_12"
                        android:layout_marginRight="@dimen/height_4"
                        android:layout_weight="1"
                        android:background="@drawable/bottom_grey"
                        android:visibility="visible"
                        tools:ignore="RtlHardcoded">

                        <TextView
                            android:id="@+id/textview_type_of_compound"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/height_3"
                            android:gravity="left"
                            android:text="@string/type_of_compound"
                            android:textColor="@color/font_text_minor_heading"
                            android:textSize="@dimen/miniut_small"
                            tools:ignore="RtlHardcoded" />

                        <Spinner
                            android:id="@+id/spinner_type_of_compound"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@null" />

                        <ImageView
                            style="@style/StyledImagedown_large"
                            android:layout_marginTop="@dimen/height_8"
                            tools:ignore="ContentDescription" />

                    </FrameLayout>


                    <LinearLayout
                        android:layout_centerVertical="true"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:layout_marginRight="@dimen/height_4"
                        android:layout_toLeftOf="@+id/type_of_compound_div"
                        android:gravity="right"
                        android:orientation="vertical"
                        tools:ignore="RtlHardcoded">

                        <TextView
                            android:id="@+id/textview_separatecompound"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_centerInParent="true"
                            android:layout_centerVertical="true"
                            android:layout_toLeftOf="@+id/id_radiogroup_separatecompound"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:paddingLeft="@dimen/height_8"
                            android:text="@string/separatecompound"
                            android:textColor="@color/font_heading"
                            android:textSize="@dimen/font_samll"
                            android:textStyle="normal"
                            tools:ignore="RtlHardcoded" />

                        <RadioGroup
                            android:id="@+id/id_radiogroup_separatecompound"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_weight="1"
                            android:gravity="right"
                            android:orientation="horizontal"
                            tools:ignore="RtlHardcoded">

                            <RadioButton
                                android:id="@+id/id_radio_separatecompound_yes"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:checked="false"
                                android:text="@string/yes" />

                            <RadioButton
                                android:id="@+id/id_radio_separatecompound_no"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/height_15"
                                android:checked="false"
                                android:text="@string/no"
                                tools:ignore="RtlHardcoded" />
                        </RadioGroup>


                    </LinearLayout>


                </RelativeLayout>


            </LinearLayout>


        </LinearLayout>


        <!-- locality for LAND -->
        <LinearLayout
            android:id="@+id/div_typeoflocality_for_land"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_3"
            android:orientation="horizontal"
            android:visibility="gone"
            android:weightSum="3"
            tools:ignore="DisableBaselineAlignment">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="0.8"
                android:background="@drawable/bottom_grey"
                tools:ignore="InefficientWeight,UselessParent">

                <TextView
                    android:id="@+id/typeoflocality_for_land"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/height_3"
                    android:gravity="left"
                    android:text="@string/typeoflocality"
