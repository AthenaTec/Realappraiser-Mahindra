                android:layout_height="match_parent"
                tools:ignore="InconsistentLayout"
                android:background="@drawable/spinner_status"
                android:popupBackground="@drawable/spinner_pop_up_dialog_bg"
                />


        </LinearLayout>

    </LinearLayout>
