                android:id="@+id/layout_compound_permissiblearea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingRight="@dimen/height_18"
                app:hintTextAppearance="@style/TextInputLayout_hint"
                tools:ignore="RtlHardcoded,RtlSymmetry"
                android:textColorHint="@color/font_texthint"
                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                >

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/editText_compound_permissiblearea"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/compound_permissiblearea"
                    android:inputType="numberDecimal"
                    android:textSize="@dimen/_11sdp"
                    android:maxLength="8" />
            </com.google.android.material.textfield.TextInputLayout>

            <ImageView
                android:id="@+id/open_calc_compound"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/icon_calculator" />
        </RelativeLayout>

        <RelativeLayout
