<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textview_land_measurement"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_gravity="left|center_vertical"
        android:layout_marginLeft="@dimen/height_3"
        android:layout_marginTop="@dimen/height_3"
        android:text="@string/land_measurement"
        android:textColor="@color/font_heading"
        android:textSize="@dimen/font_samll"
        android:textStyle="normal"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/height_2"
        android:orientation="horizontal"
        android:weightSum="6">

        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="@dimen/height_5"
            android:paddingRight="@dimen/height_5">

            <EditText
                android:id="@+id/editText_as_per_docs"
                style="@style/StyledTilEditText"
                android:hint="@string/plot_area"
                android:inputType="numberDecimal"
                android:maxLength="10" />
        </com.google.android.material.textfield.TextInputLayout>

        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/height_8"
            android:layout_marginRight="@dimen/height_8"
            android:layout_weight="1"
            android:background="@drawable/bottom_grey">

            <TextView
                android:id="@+id/textview_measurement_doc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_3"
                android:gravity="left"
                android:text="@string/units"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <Spinner
                android:id="@+id/spinnerMeasurement_doc"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null" />

            <ImageView
                style="@style/StyledImagedown_large"
                android:layout_marginTop="@dimen/height_6" />

        </FrameLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/layout_totalapartmentfloors"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingRight="@dimen/height_4">

            <EditText
                android:id="@+id/editText_totalapartmentfloors"
                style="@style/StyledTilEditText"
                android:hint="@string/totalapartmentfloors"
                android:inputType="text" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/layout_totalflatsperfloor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="@dimen/height_2"
            android:paddingRight="@dimen/height_2">

            <EditText
                android:id="@+id/editText_totalflatsperfloor"
                style="@style/StyledTilEditText"
                android:hint="@string/totalflatsperfloor"
                android:inputType="text" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/layout_flatsituatedinfloor"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:paddingLeft="@dimen/height_2"
            android:paddingRight="@dimen/height_2">

            <EditText
                android:id="@+id/editText_flatsituatedinfloor"
                style="@style/StyledTilEditText"
                android:hint="@string/flatsituatedinfloor"
                android:inputType="text" />
        </com.google.android.material.textfield.TextInputLayout>

        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/height_8"
            android:layout_marginRight="@dimen/height_5"
            android:layout_weight="1"
            android:background="@drawable/bottom_grey">

            <TextView
                android:id="@+id/textview_actualusage_flat"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_3"
                android:gravity="left"
                android:text="@string/actualusage"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_actual_usage_flat"
                style="@style/StyledTilText_multicheck"
                android:layout_gravity="left|center_vertical"
                android:gravity="left|center_vertical" />


            <ImageView
                style="@style/StyledImagedown_large"
                android:layout_marginTop="@dimen/height_6" />

        </FrameLayout>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/div_general_info_measurment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="visible">

        <TextView
            android:id="@+id/textview_generl_info_measu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/height_3"
            android:layout_marginTop="@dimen/height_8"
            android:text="@string/generl_info_measu"
            android:textColor="@color/font_heading"
            android:textSize="@dimen/font_samll"
            android:textStyle="normal" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/height_0"
            android:layout_marginTop="@dimen/height_5"
            android:orientation="horizontal"
            android:weightSum="6">

            <TextView
                android:id="@+id/textview_floor_name"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_5"
                android:text="@string/floor_name"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small"
                android:visibility="gone" />

            <TextView
                android:id="@+id/textview_stage"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_3"
                android:text="@string/stage"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_comp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_3"
                android:text="@string/comp"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_progress"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_3"
                android:text="@string/progress"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_age"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_4"
                android:text="@string/age"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_life"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_4"
                android:text="@string/life"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_measurement_act"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:text="@string/measurement"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_5"
            android:orientation="horizontal"
            android:weightSum="6">

            <EditText
                android:id="@+id/edittext_general_floor_name"
                style="@style/StyledTilEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:inputType="text"
                android:textSize="@dimen/font_samll"
                android:visibility="gone" />

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:background="@drawable/bottom_grey">

                <Spinner
                    android:id="@+id/stageSpinner"
                    style="@style/StyledTilText_multicheck"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginBottom="2dp"
                    android:background="@null"
                    android:gravity="left|center_vertical" />

                <ImageView style="@style/StyledImagedown_internal" />

            </FrameLayout>

            <EditText
                android:id="@+id/edittext_general_comp"
                style="@style/StyledTilEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:inputType="number"
                android:maxLength="3"
                android:textSize="@dimen/font_samll" />

            <EditText
                android:id="@+id/edittext_general_progress"
                style="@style/StyledTilEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:inputType="textCapWords"
                android:textSize="@dimen/font_samll" />

            <EditText
                android:id="@+id/edittext_general_age"
                style="@style/StyledTilEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:inputType="number"
                android:maxLength="2"
                android:textSize="@dimen/font_samll" />

            <EditText
                android:id="@+id/edittext_general_life"
                style="@style/StyledTilEditText"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:inputType="number"
                android:maxLength="2"
                android:textSize="@dimen/font_samll" />

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:background="@drawable/bottom_grey">

                <Spinner
                    android:id="@+id/spinner_measurement_act"
                    style="@style/StyledTilText_multicheck"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginBottom="2dp"
                    android:background="@null"
                    android:gravity="left|center_vertical" />

                <ImageView style="@style/StyledImagedown_internal" />

            </FrameLayout>


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/height_0"
            android:layout_marginTop="@dimen/height_5"
            android:orientation="horizontal"
            android:weightSum="6">

            <TextView
                android:id="@+id/textview_builduparea"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_4"
                android:text="@string/builduparea"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_saleablearea"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_1"
                android:text="@string/saleablearea"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textviewlabel_carpet_area_per"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_4"
                android:text="@string/carpet_area_per"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textviewlabel_carpetloading_type"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_4"
                android:text="@string/selected_area"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textview_carpetarea"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_4"
                android:text="@string/carpetarea"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />

            <TextView
                android:id="@+id/textviewlabel_carpetloading_per"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:gravity="left"
                android:paddingLeft="@dimen/height_4"
                android:text="@string/loading_factor"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/miniut_small" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/height_5"
            android:orientation="horizontal"
            android:weightSum="6">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1">

                <EditText
                    android:id="@+id/edittext_general_builduparea"
                    style="@style/StyledTilEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/height_0"
                    android:layout_marginRight="@dimen/height_18"
                    android:layout_weight="1"
                    android:maxLength="8"
                    android:inputType="numberDecimal"
                    android:textSize="@dimen/font_samll" />

                <ImageView
                    android:id="@+id/open_calc_builduparea"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="false"
                    android:src="@drawable/icon_calculator" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1">

                <EditText
                    android:id="@+id/edittext_general_saleablearea"
                    style="@style/StyledTilEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/height_0"
                    android:layout_marginRight="@dimen/height_18"
                    android:layout_weight="1"
                    android:maxLength="8"
                    android:inputType="numberDecimal"
                    android:textSize="@dimen/font_samll" />

                <ImageView
                    android:id="@+id/open_calc_saleablearea"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="false"
                    android:src="@drawable/icon_calculator" />
            </RelativeLayout>



            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:background="@drawable/bottom_grey">

                <TextView
                    android:id="@+id/textview_carpetloading_per"
                    style="@style/StyledTilText_multicheck"
                    android:layout_gravity="left|center_vertical"
                    android:gravity="left|center_vertical"
                    android:text="Select" />

                <ImageView style="@style/StyledImagedown_internal" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:background="@drawable/bottom_grey">

                <Spinner
                    android:id="@+id/spinner_carpetloading_type"
                    style="@style/StyledTilText_multicheck"
                    android:layout_gravity="left|center_vertical"
                    android:layout_marginBottom="2dp"
                    android:background="@null"
                    android:gravity="left|center_vertical" />

                <ImageView style="@style/StyledImagedown_internal" />

            </FrameLayout>



            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1">

                <EditText
                    android:id="@+id/edittext_general_carpetarea"
                    style="@style/StyledTilEditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/height_0"
                    android:layout_marginRight="@dimen/height_18"
                    android:layout_weight="1"
                    android:maxLength="8"
                    android:inputType="numberDecimal"
                    android:textSize="@dimen/font_samll" />

                <ImageView
                    android:id="@+id/open_calc_carpetarea"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="false"
                    android:src="@drawable/icon_calculator" />
            </RelativeLayout>


            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/height_2"
                android:layout_marginRight="@dimen/height_2"
                android:layout_weight="1"
                android:background="@drawable/bottom_grey">

                <TextView
                    android:id="@+id/textview_loading_factor"
                    style="@style/StyledTilText_multicheck"
                    android:layout_gravity="left|center_vertical"
                    android:gravity="left|center_vertical"
                    android:text="Select" />

                <ImageView style="@style/StyledImagedown_internal" />

            </FrameLayout>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/height_3"
            android:layout_marginTop="@dimen/height_3"
            android:orientation="horizontal"
            android:visibility="gone"
            android:weightSum="7">

            <TextView
                android:id="@+id/textview_average"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1.6"
                android:gravity="right"
                android:paddingRight="@dimen/height_12"
                android:text="@string/average"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/font_very_medium"
                android:visibility="gone" />

            <TextView
                android:id="@+id/textview_comp_total"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.7"
                android:gravity="left"
                android:text="@string/zero"
                android:textColor="@color/font_text"
                android:textSize="@dimen/font_very_medium" />

            <TextView
                android:id="@+id/textview_total"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.8"
                android:gravity="right"
                android:paddingRight="@dimen/height_12"
                android:text="@string/total"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/font_very_medium"
                android:visibility="invisible" />

            <TextView
                android:id="@+id/textview_doc_total"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.8"
                android:gravity="left"
                android:paddingLeft="@dimen/height_2"
                android:text="@string/zero"
                android:textColor="@color/font_text"
                android:textSize="@dimen/font_very_medium"
                android:visibility="invisible" />


            <TextView
                android:id="@+id/textview_actual_total"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.8"
                android:gravity="left"
                android:paddingLeft="@dimen/height_2"
                android:text="@string/zero"
                android:textColor="@color/font_text"
                android:textSize="@dimen/font_very_medium"
                android:visibility="invisible" />


        </LinearLayout>
    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="@dimen/height_10"
        android:layout_marginTop="@dimen/height_10"
        android:background="@color/line"></View>

    <TextView
        android:id="@+id/textview_internal_composition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/height_3"
        android:text="@string/internal_composition"
        android:textColor="@color/font_heading"
        android:textSize="@dimen/font_samll"
        android:textStyle="normal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/height_3"
        android:layout_marginTop="@dimen/height_5">


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.9"
            android:orientation="vertical">


            <TextView
                android:id="@+id/textview_floor_name_composition"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:paddingTop="@dimen/_5sdp"
                android:paddingBottom="@dimen/_5sdp"
                android:paddingStart="@dimen/_3sdp"
                android:paddingEnd="@dimen/_3sdp"
                android:text="@string/floor_name_composition"
                android:layout_gravity="center_horizontal"
                android:textColor="@color/font_text_minor_heading"
                android:textSize="@dimen/_8sdp" />

            <TextView
                android:id="@+id/textview_internal_floor_name_composition"
                style="@style/StyledTilEditText"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.9"
                android:ellipsize="end"
                android:gravity="left|bottom"
                android:hint="@string/general_floor_name"
                android:inputType="text"
                android:maxLines="1"
                android:singleLine="true"
