        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/height_3"
        android:text="@string/internal_composition"
        android:textColor="@color/font_heading"
        android:textSize="@dimen/font_samll"
        android:textStyle="normal"
        tools:ignore="RtlHardcoded" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/height_3"
        android:layout_marginTop="@dimen/height_5"
        android:orientation="horizontal"
        android:weightSum="6.8"
        tools:ignore="RtlHardcoded">

        <TextView
            android:id="@+id/textview_floor_name_composition"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="left"
            android:text="@string/floor_name_composition"
            android:textColor="@color/font_text_minor_heading"
            android:textSize="@dimen/miniut_small"
            tools:ignore="RtlHardcoded" />

        <TextView
            android:id="@+id/textview_hall_dinning"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="left"
            android:text="@string/hall_dinning"
            android:textColor="@color/font_text_minor_heading"
            android:textSize="@dimen/miniut_small"
            tools:ignore="RtlHardcoded" />

        <TextView
            android:id="@+id/textview_kitchen"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="left"
            android:text="@string/kitchen"
            android:textColor="@color/font_text_minor_heading"
            android:textSize="@dimen/miniut_small"
            tools:ignore="RtlHardcoded" />

        <TextView
            android:id="@+id/textview_bedroom"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="left"
            android:text="@string/bedroom"
            android:textColor="@color/font_text_minor_heading"
            android:textSize="@dimen/miniut_small"
            tools:ignore="RtlHardcoded" />

        <TextView
            android:id="@+id/textview_bath"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="left"
            android:text="@string/bath"
            android:textColor="@color/font_text_minor_heading"
            android:textSize="@dimen/miniut_small"
            tools:ignore="RtlHardcoded" />

        <TextView
            android:id="@+id/textview_shop_office"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:gravity="left"
            android:text="@string/shop_office"
            android:textColor="@color/font_text_minor_heading"
            android:textSize="@dimen/miniut_small"
            tools:ignore="RtlHardcoded" />

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.8"
            android:gravity="left"
            android:textColor="@color/font_text_minor_heading"
            android:textSize="@dimen/miniut_small"
            tools:ignore="RtlHardcoded" />

    </LinearLayout>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview_internal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="vertical"
        android:visibility="visible"
        tools:ignore="InconsistentLayout"></androidx.recyclerview.widget.RecyclerView>


</LinearLayout>